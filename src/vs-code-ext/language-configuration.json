{
  "comments": {
    "lineComment": ";"
  },
  "brackets": [
    ["[", "]"]
  ],
  "autoClosingPairs": [
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" }
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["[", "]"]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^[a-zA-Z0-9_-]+(\\(.*\\))?:\\s*$",
    "decreaseIndentPattern": "^(?!\\t)"
  }
}
